                    <% if (success) { %>
                        <% stores.forEach(s => { %>
                            <p class="storetop"><%= s.storename %></p>
                            <p><%= s.address %></p>
                            <p><%= s.phonenumber %></p>
                            <form class="col-3" method="post" action="#">
                                <div class="col">
                                    <input type="text" class="form-control" name="prizecode" placeholder="code">
                                </div>
                                <div class="col-8">
                                    <input type="submit" value="submit" />
