<nav class="navbar navbar-expand-md navbar-dark bg-dark">
        <ul class="navbar-nav">
            <li class="navbar-brand" href="/">Intelligentsia
                <img src="https://i.imgur.com/ob1bn4c.png" height="20" width="auto">
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="/admin/logout">Logout<span class="sr-only">(current)</span></a>
            </li>
        </ul>
    </nav>
    <div class="d-flex align-items-center justify-content-center emblem">
        <img src="https://i.imgur.com/ob1bn4c.png">
    </div>
    <div class="flex-row d-flex justify-content-between">
        <div class="col-6 justify-content-start">
            <h3><%= currentUser.name %></h3>
        </div>
        <div class="col-6 justify-content-end">
            <h3>Cafes</h3>
            <div class="card example-1 scrollbar">
                <div class="card-body storelist d-flex flex-column align-items-center">
                    <% if (success) { %>
                        <% stores.forEach(s => { %>
                            <p class="storetop"><%= s.storename %></p>
                            <p><%= s.address %></p>
                            <p><%= s.phonenumber %></p>
                            <form class="col-3" method="POST" action="/coffeerewards?_method=PUT">
                                <div class="col">
                                    <input type="text" class="form-control" id="form" name="points" placeholder="code">
                                </div>
                                <div class="col-8">
                                    <input type="submit" value="submit" />
                                </div>
                            </form>
                        <% }) %>
                    <% } %>
                </div>
            </div>
        </div>
    </div>